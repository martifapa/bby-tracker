generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Actions {
  id    Int     @id @default(autoincrement())
  emoji String
  label String
}

model Logs {
  id        Int       @id @default(autoincrement())
  datetime  DateTime
  emoji     String
  label     String
}